{
  "name": "coral-sql",
  "module": "commonjs",
  "version": "1.1.2",
  "description": "The nodejs module for building SQL by complec and related like coral.",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "build:docs": "typedoc --excludePrivate --out docs --tsconfig ./tsconfig.build.json --entryPoints src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha",
    "ncu": "ncu",
    "changeset": "changeset",
    "version": "npm run lint && npm test && changeset version",
    "publish": "changeset publish"
  },
  "keywords": [
    "sql",
    "builder"
  ],
  "author": "mizuki_r <ry.mizuki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@eslint/js": "^9.16.0",
    "@types/chai": "^5.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "chai": "^5.1.2",
    "eslint": "^9.16.0",
    "mocha": "^11.0.1",
    "npm-check-updates": "^17.1.11",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.27.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "files": [
    "dist/"
  ]
}
